{
  "name": "superlitenets/netflow-php",
  "description": "NetFlow ISP Management System - Full-Stack PHP Application",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Superlite Nets",
      "email": "admin@superlitenets.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "slim/slim": "^4.0",
    "slim/psr7": "^1.0",
    "psr/http-message": "^1.0",
    "firebase/php-jwt": "^6.0",
    "vlucas/phpdotenv": "^5.0",
    "phpmailer/phpmailer": "^6.0",
    "guzzlehttp/guzzle": "^7.0",
    "monolog/monolog": "^2.0",
    "respect/validation": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Database\\": "database/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public",
    "migrate": "php database/migrate.php",
    "seed": "php database/seed.php",
    "test": "phpunit",
    "lint": "phpcs"
  }
}
